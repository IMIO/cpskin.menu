<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    i18n:domain="cpskin.menu"
    tal:omit-tag="">

<tal:sections tal:define="portal_tabs view/portal_tabs"
     tal:condition="python: portal_tabs and not view.is_homepage"
     i18n:domain="plone">
    <p class="hiddenStructure" i18n:domain="plone" i18n:translate="heading_sections">Navigation</p>

    <ul id="portal-globalnav-cpskinmenu-tabs"
        tal:define="selected_tab python:view.selected_portal_tab"
        ><tal:tabs tal:repeat="tab portal_tabs"
        ><li tal:define="tid tab/id"
             tal:attributes="id string:portaltab-${tid};
                            class python:selected_tab==tid and 'selected' or 'plain'"
            ><a href=""
               tal:content="tab/name"
               tal:attributes="href tab/url;
                               title tab/description|nothing;">
            Tab Name
            </a></li></tal:tabs></ul>
</tal:sections>

<h5 class="hiddenStructure" i18n:domain="plone" i18n:translate="heading_sections">Sections</h5>
<ul tal:replace="structure python: view.superfish_portal_tabs().get('desktop','')" />

<a href="#" id="search-btn">
    Search
</a>
<div id="mobnav-btn">Menu de navigation</div>
<div id="mobile-first-level-wrapper"></div>
<div id="advanced-breadcrumbs"></div>
<ul tal:replace="structure python: view.superfish_portal_tabs().get('mobile','')" />
<div class="visualClear"><!-- --></div>

</html>
